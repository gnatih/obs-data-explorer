<tr>
    <td class="country"><span>{{country}}</span></td>
    <td class="flag"><img src="images/flags/{{alpha2}}.GIF"/></td>
    <td class="score">{{score}}%</td>
    <td class="bar">
      <div class="percentbar" title="<div class='percentbar-tooltip'>
          <div class='heading'>Survey Scores</div>
          <table>
            <tr><td class='c1'>A:</td><td class='c2'>{{a}}</td><td class='c3'>/</td><td class='c4'>{{total}}</td></tr>
            <tr><td class='c1'>B:</td><td class='c2'>{{b}}</td><td class='c3'>/</td><td class='c4'>{{total}}</td></tr>
            <tr><td class='c1'>C:</td><td class='c2'>{{c}}</td><td class='c3'>/</td><td class='c4'>{{total}}</td></tr>
            <tr><td class='c1'>D:</td><td class='c2'>{{d}}</td><td class='c3'>/</td><td class='c4'>{{total}}</td></tr>
            <tr><td class='c1'>E:</td><td class='c2'>{{e}}</td><td class='c3'>/</td><td class='c4'>{{total}}</td></tr>
          </table>
          </div>">
          <div class="a" style="left: 0%; width: {{a_width}}%"></div>
          <div class="b" style="left: {{b_left}}%; width: {{b_width}}%"></div>
          <div class="c" style="left: {{c_left}}%; width: {{c_width}}%"></div>
          <div class="d" style="left: {{d_left}}%; width: {{d_width}}%"></div>
          <div class="e" style="left: {{e_left}}%; width: {{e_width}}%"></div>
        </div>
    </td>
</tr>
